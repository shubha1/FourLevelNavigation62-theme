<look-and-feel>
	<compatibility>
		<version>6.2.10+</version>
	</compatibility>
	<theme id="fourLevelNavigation62" name="FourLevelNavigation62" >
		<color-scheme id="1" name="Orange Sun Rise">
			<css-class>orange</css-class>
			<color-scheme-images-path>${images-path}/color_schemes/${css-class}</color-scheme-images-path>
		</color-scheme>
		<color-scheme id="2" name="Blue Sky">
			<css-class>blue</css-class>
			<color-scheme-images-path>${images-path}/color_schemes/${css-class}</color-scheme-images-path>
		</color-scheme>
		<color-scheme id="3" name="Green Grass">
			<css-class>green</css-class>
			<color-scheme-images-path>${images-path}/color_schemes/${css-class}</color-scheme-images-path>
		</color-scheme>
	</theme>
</look-and-feel>